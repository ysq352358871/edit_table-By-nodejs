<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
    <link rel="stylesheet" href="/css/list.css">
    <script src="/js/jquery.min.js"></script>
    <script src="/js/list.js"></script>
</head>
<body>
<div class="box">
    <div class="header">学籍管理系统</div>
    <table cellspacing="0";cellpadding="0">
        <tr>
            <th>学号</th>
            <th>姓名</th>
            <th>性别</th>
            <th>年龄</th>
            <th>操作</th>
        </tr>
        <% result.forEach(function (result) {%>
        <tr attr="<%=result.id%>">
            <td class="first" attr="num"><%=result.num%></td>
            <td class="first" attr="name"><%=result.name%></td>
            <td class="first" attr="sex"><%=result.sex%></td>
            <td class="first" attr="age"><%=result.age%></td>
            <td><div class='del' >删除</div></td>
        </tr>

        <%})%>

    </table>
    <div class="btn">添加</div>

</div>
<!--<form action="/index/" method="post" >-->
    <!--<input type="text" name="name">-->
    <!--<input type="submit" value="提交">-->
<!--</form>-->
</body>
</html>
